{
    "Iteration title": {
        "prefix": "iteration",
        "body": [
            "IT#"
        ],
        "description": "Iteration marker"
    },
    "User story title": {
        "prefix": "edit user story",
        "body": [
            "US#"
        ],
        "description": "User Story marker"
    },
    "Create User story": {
        "prefix": "create user story",
        "body": [
            "US#new - ${1:title}"
        ],
        "description": "User Story marker"
    },
    "Task": {
        "prefix": "new task",
        "body": [
            "- ${1:title} - ${2:estimation}h"
        ],
        "description": "Task"
    }
}